<?xml version="1.0" encoding="UTF-8"?>
<assessmentItem xmlns="http://www.imsglobal.org/xsd/imsqti_v2p1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.imsglobal.org/xsd/imsqti_v2p1 http://www.imsglobal.org/xsd/qti/qtiv2p1/imsqti_v2p1.xsd" identifier="test_OneInColTable_abbr_example" title="OneInColTable" adaptive="false" timeDependent="false">
  <responseDeclaration identifier="RESPONSE" cardinality="multiple" baseType="directedPair">
    <correctResponse>
      <value>Spielen_ShrbkF _True</value>
      <value>Fliegen_SbtadF _False</value>
      <value>Fliegen_SbtadF _mit</value>
      <value>Schicken_SmbeB! _True</value>
    </correctResponse>
    <mapping defaultValue="0">
      <mapEntry mapKey="Spielen_ShrbkF _True" mappedValue="1"/>
      <mapEntry mapKey="Fliegen_SbtadF _False" mappedValue="1"/>
      <mapEntry mapKey="Fliegen_SbtadF _mit" mappedValue="1"/>
      <mapEntry mapKey="Schicken_SmbeB! _True" mappedValue="1"/>
      <mapEntry mapKey="Spielen_ShrbkF _False" mappedValue="-0.5"/>
      <mapEntry mapKey="Spielen_ShrbkF _mit" mappedValue="-0.5"/>
      <mapEntry mapKey="Spielen_ShrbkF _ein" mappedValue="-0.5"/>
      <mapEntry mapKey="Fliegen_SbtadF _True" mappedValue="-0.5"/>
      <mapEntry mapKey="Fliegen_SbtadF _ein" mappedValue="-0.5"/>
      <mapEntry mapKey="Schicken_SmbeB! _False" mappedValue="-0.5"/>
      <mapEntry mapKey="Schicken_SmbeB! _mit" mappedValue="-0.5"/>
      <mapEntry mapKey="Schicken_SmbeB! _ein" mappedValue="-0.5"/>
    </mapping>
  </responseDeclaration>
  <outcomeDeclaration identifier="SCORE" cardinality="single" baseType="float">
    <defaultValue>
      <value>0</value>
    </defaultValue>
  </outcomeDeclaration>
  <outcomeDeclaration identifier="MAXSCORE" cardinality="single" baseType="float">
    <defaultValue>
      <value>4</value>
    </defaultValue>
  </outcomeDeclaration>
  <outcomeDeclaration identifier="MINSCORE" cardinality="single" baseType="float">
    <defaultValue>
      <value>0</value>
    </defaultValue>
  </outcomeDeclaration>
  <itemBody>
    <p>Choose the correct order in the multiplication table</p>
    <matchInteraction responseIdentifier="RESPONSE" shuffle="true" maxAssociations="4">
      <simpleMatchSet>
        <simpleAssociableChoice identifier="Spielen_ShrbkF" fixed="false" matchMax="4">Spielen Sie hier bitte kein Fahrrad</simpleAssociableChoice>
        <simpleAssociableChoice identifier="Fliegen_SbtadF" fixed="false" matchMax="4">Fliegen Sie bitte an dem Flugzeug</simpleAssociableChoice>
        <simpleAssociableChoice identifier="Schicken_SmbeB!" fixed="false" matchMax="4">Schicken Sie mir bitte einen Brief!</simpleAssociableChoice>
      </simpleMatchSet>
      <simpleMatchSet>
        <simpleAssociableChoice identifier="_True" fixed="false" matchMax="3">True</simpleAssociableChoice>
        <simpleAssociableChoice identifier="_False" fixed="false" matchMax="3">False</simpleAssociableChoice>
        <simpleAssociableChoice identifier="_mit" fixed="false" matchMax="3">mit</simpleAssociableChoice>
        <simpleAssociableChoice identifier="_ein" fixed="false" matchMax="3">ein</simpleAssociableChoice>
      </simpleMatchSet>
    </matchInteraction>
  </itemBody>
  <responseProcessing>
    <responseCondition>
      <responseIf>
        <not>
          <isNull>
            <variable identifier="RESPONSE"/>
          </isNull>
        </not>
        <setOutcomeValue identifier="SCORE">
          <sum>
            <variable identifier="SCORE"/>
            <mapResponse identifier="RESPONSE"/>
          </sum>
        </setOutcomeValue>
      </responseIf>
    </responseCondition>
    <responseCondition>
      <responseIf>
        <gt>
          <variable identifier="SCORE"/>
          <variable identifier="MAXSCORE"/>
        </gt>
        <setOutcomeValue identifier="SCORE">
          <variable identifier="MAXSCORE"/>
        </setOutcomeValue>
      </responseIf>
    </responseCondition>
    <responseCondition>
      <responseIf>
        <lt>
          <variable identifier="SCORE"/>
          <variable identifier="MINSCORE"/>
        </lt>
        <setOutcomeValue identifier="SCORE">
          <variable identifier="MINSCORE"/>
        </setOutcomeValue>
      </responseIf>
    </responseCondition>
  </responseProcessing>
</assessmentItem>
